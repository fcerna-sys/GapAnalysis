  "keywords": [
    "team",
    "miembro",
    "equipo",
    "molecule"
  ],
  "style": "file:./style.css",
  "render": "file:./render.php",
  "example": {
    "attributes": {
      "name": "Nombre",
      "role": "Rol",
      "avatarUrl": "https://via.placeholder.com/128"
    }
  },
  "attributes": {
    "name": {
